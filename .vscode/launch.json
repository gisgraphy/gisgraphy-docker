                "cwd": "$git clone https://github.com/gisgraphy/gisgraphy-docker.git && cd gisgraphy-docker && ./install-docker.sh && cd base && ./get_from_dockerhub.sh
{workspaceFolder}",
                "stopAtEntry": false,
